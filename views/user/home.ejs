  <!-- Plugins CSS File -->
  <link rel="stylesheet" href="newOne/css/bootstrap.min.css">
  <link rel="stylesheet" href="newOne/css/plugins/owl-carousel/owl.carousel.css">
  <link rel="stylesheet" href="newOne/css/plugins/magnific-popup/magnific-popup.css">
